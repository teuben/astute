#! /usr/bin/env python
#
#   show what's inside a BDP
#   
#   for now only for cPickle'd versions
#

# file = 'test00.p'

import sys, os
import admit1 as admit


if __name__ == "__main__":
    if len(sys.argv) < 2:
	print 'Need a (pickle) filename'
	sys.exit(0)
    file = sys.argv[1]
    a = admit.ADMIT()
    b = a.pload(file)
    b.info()
